<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguider">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/lo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Sign_In"
            android:label="Sign In"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".List_of_Hotel"
            android:label="List of Hotel"
            android:parentActivityName=".Home_page"/>
        <activity
            android:name=".Home_page"
            android:label="home page" />
        <activity
            android:name=".online"
            android:parentActivityName=".List_of_Hotel"
            android:label="web booking" />
        <activity android:name=".Combination"
            android:parentActivityName=".Home_page"/>
        <activity android:name=".ListOfTemple"
            android:parentActivityName=".Home_page"/>
        <activity android:name=".ListOfForts"
            android:parentActivityName=".Home_page"></activity>
    </application>

</manifest>